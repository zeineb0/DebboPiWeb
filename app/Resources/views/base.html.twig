    <script>
        $("document").ready(function () {

            var listes = function(){
                $("#listNotif").append(

                    '<div class="blockNotification" > <label style="width: 220px">Notification</label>' +
                    '<table class="listNotification table"   id="block_notif" >'+
                    '</table></div>'

                );
                $.ajax({
                    type:"POST",
                    url:"{{ path('blog_index') }}",
                    success: function(reponse){
                        $("#count").text(reponse[0]['count']);
                        if (!reponse.length)
                        {
                            $("#block_notif").append(
                                '<tr>' +
                                '<td>Pas de Notification</td>' +
                                '</tr>'
                            );
                        }
                        else {
                            $.each(reponse, function () {
                                $("#block_notif").append(

                                    '<tr  >'+
                                    '<td  style="float: left; "><b>' + this.auteur +'</b> à '+ this.title +
                                    '<p style=" white-space: nowrap;width: 200px;overflow: hidden;' +
                                    'text-overflow: ellipsis;">' + this.description + '</p>' +
                                    '<p style="color:blue;">' + this.date + '</p></td>' +
                                    '</tr>'
                                )

                            });

                        }

                    }
                });

            };
            $("#notif").click(function () {
                $("#listNotif").toggle();
            });
            $("#listNotif").load(listes());
            setInterval(function () {
                $("#listNotif").empty();
                $("#listNotif").load(listes());
            },60000);

        });


    </script>
